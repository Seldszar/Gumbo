{
  "manifest_version": 3,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "default_locale": "en",
  "version": "0.0.0",
  "permissions": [
    "alarms",
    "background",
    "notifications",
    "storage",
    "unlimitedStorage"
  ],
  "host_permissions": [
    "https://gumbo.seldszar.fr/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "48": "icon-48.png",
    "96": "icon-96.png"
  },
  "action": {
    "default_title": "__MSG_extensionName__",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon-16.png",
      "32": "icon-32.png"
    }
  },
  "commands": {
    "_execute_action": {}
  },
  "options_page": "settings.html"
}
